<header role="banner">
	<%= link_to 'Three Word Poems!',
		category_url( Category.first ),
		title: "Return to the homepage, and show a poem from the Top Ten poems."
		%>
		<% flash.each do |message_type, message| %>
			<%= content_tag(:p, message, class: "alert alert-#{message_type}") %>
		<% end %>
</header>